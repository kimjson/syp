{
  "name": "syp",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "predev": "vercel env pull --yes",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "setenv": "dotenv -e .env",
    "prisma:generate": "dotenv -e .env -- prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:introspect": "dotenv -e .env -- prisma introspect --schema ./src/prisma/schema.prisma",
    "prisma:migrate:save": "dotenv -e .env -- prisma migrate save --experimental --schema ./src/prisma/schema.prisma",
    "prisma:migrate:up": "dotenv -e .env -- prismamigrate up --experimental --schema ./src/prisma/schema.prisma",
    "prisma:studio": "dotenv -e .env -- prisma studio --experimental --schema ./src/prisma/schema.prisma",
    "prebuild": "yarn prisma:generate"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@emotion/core": "^10.0.34",
    "@emotion/styled": "^10.0.27",
    "@prisma/cli": "^2.8.1",
    "@prisma/client": "^2.8.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "dotenv-cli": "^4.0.0",
    "ky": "^0.23.0",
    "ky-universal": "^0.8.1",
    "metascraper": "^5.13.2",
    "metascraper-date": "^5.14.2",
    "metascraper-description": "^5.13.2",
    "metascraper-title": "^5.13.2",
    "metascraper-url": "^5.13.2",
    "mysql": "^2.18.1",
    "next": "^9.5.3",
    "next-auth": "^3.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "sweetalert2": "^9.17.1",
    "swr": "^0.3.0",
    "typeorm": "^0.2.25",
    "url-parse": "^1.4.7",
    "vercel": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "typescript": "3.9.3"
  },
  "license": "ISC"
}
